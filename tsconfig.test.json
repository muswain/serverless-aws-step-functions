{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true
  },
  "ts-node": {
    "files": true
  },
  "include": ["src/**/*.ts", "./typings/**/*.d.ts"],
  "exclude": ["node_modules/**/*", ".serverless/**/*", ".webpack/**/*", "_warmup/**/*", ".vscode/**/*"]
}
